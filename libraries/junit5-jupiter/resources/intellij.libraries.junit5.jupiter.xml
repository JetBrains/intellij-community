<!-- we have intellij.libraries.junit5.vintage as runtime dep to ensure org.junit.vintage.engine.VintageTestEngine appears before org.junit.jupiter.engine.JupiterTestEngine after the topological sort -->
<idea-plugin visibility="internal">
  <dependencies>
    <!-- region Generated dependencies - run `Generate Product Layouts` to regenerate -->
    <module name="intellij.libraries.junit5"/>
    <module name="intellij.libraries.junit5.launcher"/>
    <!-- endregion -->

    <module name="intellij.libraries.junit5.vintage"/>
  </dependencies>
</idea-plugin>