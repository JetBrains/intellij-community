{
  "$schema": "../../web-types-gen/resources/web-types.json",
  "name": "framework-web-types",
  "js-types-syntax": "typescript",
  "version": "0.1.0",
  "framework": "@polymer/polymer",
  "framework-config": {
    "enable-when": {
      "node-packages": [
        "@polymer/polymer"
      ]
    },
    "canonical-names": {
      "/js/properties": "kebab-case"
    },
    "match-names": {
      "/js/properties": [
        "as-is"
      ]
    },
    "name-variants": {
      "/js/properties": [
        "kebab-case"
      ]
    }
  },
  "contributions": {
    "html": {
      "attributes": [
        {
          "name": "Properties",
          "description": "Properties expression",
          "value": {
            "kind": "expression",
            "type": "any"
          },
          "pattern": {
            "items": [
              "/js/properties"
            ],
            "template": [
              "#item:property name"
            ]
          }
        }
      ]
    }
  }
}