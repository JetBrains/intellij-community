<keymap version="1" name="Default for XWin" disable-mnemonics="false" parent="$default">
  <action id="FileChooser.GoToRoot">
    <keyboard-shortcut first-keystroke="control SLASH"/>
  </action>
  <action id="RunToCursor">
    <keyboard-shortcut first-keystroke="shift alt 9"/>
  </action>
  <action id="ForceRunToCursor">
    <keyboard-shortcut first-keystroke="control alt 9"/>
  </action>
  <action id="QuickEvaluateExpression">
    <keyboard-shortcut first-keystroke="control alt 8"/>
    <mouse-shortcut keystroke="control shift alt button1"/>
  </action>
  <action id="ShowUsages">
    <keyboard-shortcut first-keystroke="control alt 7"/>
  </action>
  <action id="ShowSettingsAndFindUsages">
    <keyboard-shortcut first-keystroke="control shift alt 7"/>
  </action>
  <action id="SwitchCoverage">
    <keyboard-shortcut first-keystroke="control alt 6"/>
  </action>
  <action id="Images.EditExternally">
    <keyboard-shortcut first-keystroke="control alt 4"/>
  </action>
  <action id="ShowFilePath">
    <keyboard-shortcut first-keystroke="control alt shift 2"/>
  </action>
  <action id="XDebugger.AttachToProcess">
    <keyboard-shortcut first-keystroke="control alt 5"/>
  </action>
  <action id="ZoomInIdeAction">
    <keyboard-shortcut first-keystroke="shift control alt EQUALS"/>
  </action>
  <action id="ZoomOutIdeAction">
    <keyboard-shortcut first-keystroke="shift control alt MINUS"/>
  </action>
  <action id="ResetIdeScaleAction">
    <keyboard-shortcut first-keystroke="shift control alt 0"/>
  </action>
  <action id="ActivateUnitTestsToolWindow"/>
  <action id="ActivateNuGetToolWindow"/>
</keymap>
