{
  "groups": [
    {
      "id": "ui.fonts",
      "builds": [
        {
          "from": "191.4811"
        }
      ],
      "rules": {
        "event_id": [
          "IDE.editor.font.name[{enum#font}]",
          "UI.font.size[{regexp#integer}]",
          "Presentation.mode.font.size[{regexp#integer}]",
          "Editor.font.size[{regexp#integer}]",
          "IDE.editor.font.size[{regexp#integer}]",
          "Console.font.size[{regexp#integer}]",
          "QuickDoc.font.size[{regexp#integer}]",
          "UI.font.name[{enum#font}]",
          "Editor.font.name[{enum#font}]",
          "Console.font.name[{enum#font}]"
        ],
        "enums": {
          "font": [
            ".SFNSText-Regular",
            "Lucida_Grande",
            "Ubuntu",
            ".SF_NS_Text",
            "Segoe_UI",
            "Noto_Sans_Regular",
            "Cantarell_Regular",
            "Dialog.plain",
            "Microsoft_YaHei_UI",
            "Noto_Sans",
            "DejaVu_Sans",
            "Tahoma",
            "Dialog",
            "Microsoft_YaHei",
            "Cantarell",
            "Fira_Code",
            "Source_Code_Pro",
            "DejaVu_Sans_Mono",
            "Consolas",
            "Roboto",
            "LucidaGrande",
            "Liberation_Sans",
            "Open_Sans",
            "Roboto-Regular",
            "Monospaced",
            "Fira_Code_Retina"
          ]
        }
      }
    }
  ],
  "rules": {
    "regexps": {
      "boolean": "true|false",
      "integer": "\\d+(\\+)?",
      "version": "\\d+(\\.\\d+(\\.\\d+(\\.\\d+)?)?)?"
    }
  }
}