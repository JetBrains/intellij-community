<!--
  This file contains extensions, actions, services, etc. without v2 dependencies,
  so it can be included in Analyzer dependencies where V2 is not yet supported.

  All new extensions, actions, services, extension points, etc. should be added to this file.
-->
<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="com.intellij.xdebugger.consoleViewDataConverter"
                    interface="com.intellij.platform.debugger.impl.rpc.XDebuggerConsoleViewConverter"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.xdebugger.descriptorStateManager"
                    interface="com.intellij.platform.debugger.impl.shared.FrontendDescriptorStateManagerExtension"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.xdebugger.managerProxy" interface="com.intellij.platform.debugger.impl.shared.proxy.XDebugManagerProxy"
                    dynamic="true"/>
    <extensionPoint qualifiedName="com.intellij.xdebugger.monolithAccessPoint"
                    interface="com.intellij.platform.debugger.impl.shared.XDebuggerMonolithAccessPoint"
                    dynamic="true"/>
  </extensionPoints>

  <extensions defaultExtensionNs="com.intellij">
    <statistics.validation.customValidationRule implementation="com.intellij.platform.debugger.impl.shared.FrameTypeValidator"/>
    <statistics.counterUsagesCollector implementationClass="com.intellij.platform.debugger.impl.shared.XDebuggerActionsCollector"/>
    <xdebugger.customXDescriptorSerializerProvider implementation="com.intellij.xdebugger.impl.mixedmode.XMixedModeExecutionStackDescriptorSerializerProvider"/>
  </extensions>
</idea-plugin>
