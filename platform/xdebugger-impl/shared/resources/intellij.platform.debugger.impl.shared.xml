<idea-plugin visibility="public">
  <dependencies>
    <module name="intellij.platform.debugger.impl.rpc"/>
  </dependencies>

  <extensionPoints>
    <extensionPoint name="xdebugger.consoleViewDataConverter"
                    interface="com.intellij.platform.debugger.impl.rpc.XDebuggerConsoleViewConverter"
                    dynamic="true"/>
    <extensionPoint name="xdebugger.descriptorStateManager"
                    interface="com.intellij.platform.debugger.impl.shared.FrontendDescriptorStateManagerExtension"
                    dynamic="true"/>
  </extensionPoints>
</idea-plugin>