<idea-plugin use-idea-classloader="true">
  <id>com.intellij.workspace.ide.tests</id>
  <name>com.intellij.workspace.ide.tests</name>

  <depends>com.intellij.modules.platform</depends>

  <module-components>
    <component>
      <implementation-class>com.intellij.workspace.jps.TestModuleComponent</implementation-class>
    </component>
  </module-components>

  <extensions defaultExtensionNs="com.intellij">
    <moduleExtension implementation="com.intellij.workspace.jps.TestModuleExtension"/>
  </extensions>

</idea-plugin>
