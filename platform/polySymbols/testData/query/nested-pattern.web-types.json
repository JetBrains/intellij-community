{
  "$schema": "../../web-types-gen/resources/web-types.json",
  "framework": "vue",
  "name": "vue",
  "version": "3.0.0",
  "js-types-syntax": "typescript",
  "description-markup": "markdown",
  "framework-config": {
    "enable-when": {
      "node-packages": [
        "vue",
        "@vue/cli"
      ],
      "file-extensions": [
        "vue"
      ],
      "ide-libraries": [
        "vue"
      ]
    }
  },
  "contributions": {
    "html": {
      "elements": [
        {
          "name": "Vue component",
          "pattern": {
            "items": "/html/vue-components"
          },
          "attributes": [
            {
              "name": "Component property",
              "pattern": {
                "items": "props"
              }
            }
          ]
        }
      ],
      "vue-components": [
        {
          "name": "Special-component",
          "pattern": {
            "items": "/html/attributes",
            "template": [
              "bar-",
              "#item"
            ]
          }
        }
      ],
      "attributes": [
        {
          "name": "foo"
        }
      ]
    }
  }
}