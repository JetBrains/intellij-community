{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://jetbrains.com/events-test-scheme.schema.json",
  "type": "object",
  "title": "Feature Usage Statistics Test Scheme",
  "description": "Feature Usage Statistics Test Scheme",
  "required": [
    "event_id",
    "event_data"
  ],
  "properties": {
    "event_id": {
      "$id": "#/properties/event_id",
      "type": "array",
      "items": {
        "$id": "#/properties/event_id/items",
        "type": "string",
        "examples": [
          "{util#action}",
          "{enum:action.invoked|custom.action.invoked}"
        ]
      },
      "minItems": 1
    },
    "event_data": {
      "$id": "#/properties/event_data",
      "type": "object",
      "default": {},
      "examples": [
        {
          "plugin": [
            "{util#plugin}"
          ]
        }
      ],
      "patternProperties": {
        ".*": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "examples": [
            "{util#action}"
          ],
          "minItems": 1
        }
      },
      "enums": {
        "type": "object",
        "additionalProperties": false,
        "patternProperties": {
          ".*": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "minItems": 1
          }
        }
      },
      "regexps": {
        "type": "object",
        "additionalProperties": false,
        "patternProperties": {
          ".*": {
            "type": "string"
          }
        }
      }
    }
  }
}