<idea-plugin package="com.intellij.platform.ae.database">
  <extensionPoints>
    <extensionPoint qualifiedName="com.intellij.platform.ae.database.fusBasedCounterUserActivity"
                    interface="com.intellij.platform.ae.database.baseEvents.FusBasedCounterUserActivity$Factory"
                    dynamic="false"
    />
  </extensionPoints>

  <extensions defaultExtensionNs="com.intellij">
    <!-- TODO: current usage is allowed, but will be removed after proper API from FUS team -->
    <applicationService
      serviceImplementation="com.intellij.platform.ae.database.baseEvents.AddStatisticsEventLogListenerTemporary"
      preload="true"
    />
  </extensions>
</idea-plugin>