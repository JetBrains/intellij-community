<idea-plugin>
  <!-- region Generated dependencies - run `Generate Product Layouts` to regenerate -->
  <dependencies>
    <module name="intellij.libraries.bouncy.castle.provider"/>
    <module name="intellij.libraries.caffeine"/>
    <module name="intellij.libraries.fastutil"/>
    <module name="intellij.libraries.guava"/>
    <module name="intellij.libraries.kotlinx.coroutines.core"/>
    <module name="intellij.libraries.snakeyaml"/>
    <module name="intellij.platform.core"/>
    <module name="intellij.platform.core.ui"/>
    <module name="intellij.platform.ide.core"/>
    <module name="intellij.platform.projectModel"/>
    <module name="intellij.platform.projectModel.impl"/>
    <module name="intellij.platform.util.ui"/>
  </dependencies>
  <!-- endregion -->
  <extensions defaultExtensionNs="com.intellij">
    <applicationService serviceInterface="com.intellij.ide.passwordSafe.PasswordSafe" open="true"
                        serviceImplementation="com.intellij.ide.passwordSafe.impl.PasswordSafeImpl"
                        testServiceImplementation="com.intellij.ide.passwordSafe.impl.TestPasswordSafeImpl"/>

    <applicationConfigurable parentId="preferences.general" instance="com.intellij.credentialStore.PasswordSafeConfigurable"
                             bundle="messages.CredentialStoreBundle"
                             key="password.safe.configurable"
                             id="application.passwordSafe"/>

    <applicationService serviceInterface="com.intellij.credentialStore.CredentialStoreUiService"
                        serviceImplementation="com.intellij.credentialStore.CredentialStoreUiServiceImpl"/>
  </extensions>
</idea-plugin>