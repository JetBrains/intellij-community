<idea-plugin>
  <name>Built-in Server Tests</name>
  <id>org.jetbrains.platform.builtInServer.tests</id>

  <!-- otherwise plugin will be not loaded -->
  <depends>com.intellij.modules.xml</depends>

  <extensions defaultExtensionNs="org.jetbrains">
    <binaryRequestHandler implementation="org.jetbrains.ide.BinaryRequestHandlerTest$MyBinaryRequestHandler"/>
  </extensions>
</idea-plugin>