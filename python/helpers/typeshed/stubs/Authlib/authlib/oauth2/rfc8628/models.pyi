class DeviceCredentialMixin:
    def get_client_id(self): ...
    def get_scope(self): ...
    def get_user_code(self): ...
    def is_expired(self): ...

class DeviceCredentialDict(dict[str, object], DeviceCredentialMixin):
    def get_client_id(self): ...
    def get_scope(self): ...
    def get_user_code(self): ...
    def get_nonce(self): ...
    def get_auth_time(self): ...
    def is_expired(self): ...
