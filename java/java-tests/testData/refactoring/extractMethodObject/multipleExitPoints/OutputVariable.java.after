class Foo {
  String foo() {
      Inner inner = new Inner().invoke();
      String var = inner.getVar();
      if (var == null) return null;
      System.out.println(var);
  }

    private class Inner {
        private boolean myResult;
        private String var;

        boolean is() {
            return myResult;
        }

        public String getVar() {
            return var;
        }

        public Inner invoke() {
            var = "";
            if (var == null) {
                myResult = true;
                return this;
            }
            myResult = false;
            return this;
        }
    }
}