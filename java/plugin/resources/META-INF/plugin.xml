<idea-plugin allow-bundled-update="true">
  <id>com.intellij.java</id>
  <name>Java</name>
  <description><![CDATA[
    Adds support for the Java language, including Java projects, language syntax highlighting, inspections, and all other coding assistance features.
  ]]></description>

  <vendor>JetBrains</vendor>
  <category>Languages</category>
  <resource-bundle>messages.ActionsBundle</resource-bundle>

  <dependencies>
    <plugin id="com.intellij.modules.lang"/>
    <plugin id="com.intellij.modules.vcs"/>
    <plugin id="com.intellij.modules.xdebugger"/>
    <plugin id="com.intellij.modules.xml"/>
    <plugin id="com.intellij.modules.java-capable"/>
    <module name="intellij.platform.structuralSearch"/>
    <module name="intellij.platform.vcs.impl.lang"/>
    <module name="intellij.spellchecker"/>
    <module name="intellij.xml.dom.impl"/>
    <module name="intellij.regexp"/>
  </dependencies>

  <module value="com.intellij.modules.java"/>

  <content namespace="jetbrains">
    <module name="intellij.java.syntax" loading="embedded"/>
    <module name="intellij.java.frontback.psi" loading="embedded"/>
    <module name="intellij.java.frontback.psi.impl" loading="embedded"/>
    <module name="intellij.java.psi" loading="embedded"/>
    <module name="intellij.java.psi.impl" loading="embedded"/>
    <module name="intellij.java.frontback.impl" loading="embedded"/>

    <module name="intellij.java.indexing" loading="embedded"/>
    <module name="intellij.platform.uast" loading="embedded"/>
    <module name="intellij.platform.uast.ide" loading="embedded"/>
    <module name="intellij.java.uast" loading="embedded"/>
    <module name="intellij.java.compiler" loading="embedded"/>
    <module name="intellij.java.analysis" loading="embedded"/>
    <module name="intellij.java" loading="embedded"/>
    <module name="intellij.java.resources" loading="embedded"/>
    <module name="intellij.java.execution" loading="embedded"/>
    <module name="intellij.java.remoteServers" loading="optional"/>
    <module name="intellij.jvm.analysis" loading="embedded"/>
    <module name="intellij.jsp" loading="embedded"/>
    <module name="intellij.java.resources.en" loading="embedded"/>
    <module name="intellij.java.debugger" loading="embedded"/>
    <module name="intellij.java.codeserver.core" loading="embedded"/>
    <module name="intellij.java.codeserver.highlighting" loading="embedded"/>

    <module name="intellij.jsp.spi" loading="embedded"/>
    <module name="intellij.java.indexing.impl" loading="embedded"/>
    <module name="intellij.java.analysis.impl" loading="embedded"/>
    <module name="intellij.java.structureView" loading="embedded"/>

    <module name="intellij.java.impl" loading="embedded"/>
    <module name="intellij.java.impl.refactorings" loading="embedded"/>
    <module name="intellij.java.compiler.impl" loading="embedded"/>
    <module name="intellij.java.typeMigration" loading="embedded"/>
    <module name="intellij.java.impl.inspections" loading="embedded"/>
    <module name="intellij.jvm.analysis.refactoring" loading="embedded"/>
    <module name="intellij.jvm.analysis.quickFix" loading="embedded"/>
    <module name="intellij.uiDesigner" loading="embedded"/>
    <module name="intellij.java.manifest" loading="embedded"/>
    <module name="intellij.java.ui" loading="embedded"/>
    <module name="intellij.java.execution.impl" loading="embedded"/>
    <module name="intellij.java.debugger.impl" loading="embedded"/>
    <module name="intellij.java.debugger.memory.agent" loading="embedded"/>
    <module name="intellij.java.remoteServers.impl" loading="optional"/>
    <module name="intellij.java.uast.ide"  loading="embedded"/>
    <module name="intellij.java.backend" required-if-available="intellij.platform.backend" />


    <module name="intellij.java.debugger.impl.shared" loading="embedded"/>
    <module name="intellij.java.execution.impl.shared" loading="embedded"/>
    <module name="intellij.java.structuralSearch"/>
    <module name="intellij.java.featuresTrainer"/>
    <module name="intellij.java.performancePlugin" loading="optional"/>
    <module name="intellij.java.unscramble"/>
    <module name="intellij.java.vcs"/>
    <module name="intellij.java.terminal" loading="optional"/>
    <module name="intellij.jvm.analysis.impl" required-if-available="intellij.platform.backend"/>
    <module name="intellij.java.debugger.impl.frontend"/>
    <module name="intellij.java.debugger.impl.backend" loading="embedded"/>
    <module name="intellij.java.execution.impl.frontend"/>
    <module name="intellij.java.execution.impl.backend" required-if-available="intellij.platform.backend"/>
    <module name="intellij.java.ultimate.icons" loading="optional"/>
    <module name="intellij.java.langInjection"/>
    <module name="intellij.java.langInjection.jps"/>
    <module name="intellij.java.duplicates"/>
    <module name="intellij.java.duplicates.analysis"/>
    <module name="intellij.java.duplicatesDetection"/>
    <module name="intellij.java.jam.impl"/>
    <module name="intellij.java.jam"/>
    <module name="intellij.profiler.ultimate"/>
    <module name="intellij.jvm.dfa.analysis.ui"/>
    <module name="intellij.java.copyright"/>
    <module name="intellij.java.promo"/>
    <module name="intellij.debugger.streams.shared" loading="required"/>
  </content>
</idea-plugin>