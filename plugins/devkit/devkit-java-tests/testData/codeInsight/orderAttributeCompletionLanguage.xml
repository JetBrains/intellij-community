<idea-plugin>
  <id>com.intellij</id>

  <extensionPoints>
    <!--suppress PluginXmlValidity -->
    <extensionPoint name="myEp7"
                    beanClass="com.intellij.codeInsight.completion.CompletionContributorEP"/>
  </extensionPoints>

  <extensions defaultExtensionNs="com.intellij">
    <myEp7 id="javaLanguage1" language="JAVA"/>
    <!--suppress PluginXmlValidity -->
    <myEp7 id="anyLanguage" language="any"/>
    <myEp7 id="withoutLanguage"/>
    <myEp7 id="xmlLanguage" language="XML"/>
    <myEp7 id="javaLanguage2" language="JAVA" order="before <caret>"/>
  </extensions>
</idea-plugin>