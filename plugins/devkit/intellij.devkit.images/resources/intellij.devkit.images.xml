  <dependencies>
    <module name="intellij.devkit.core"/>
    <plugin id="com.intellij.platform.images"/>
  </dependencies>
  <extensions defaultExtensionNs="com.intellij">
    <images.themeFilter implementation="org.jetbrains.idea.devkit.images.DefaultThemeFilter"/>
    <images.themeFilter implementation="org.jetbrains.idea.devkit.images.DefaultHiDPIThemeFilter"/>
    <images.themeFilter implementation="org.jetbrains.idea.devkit.images.DarculaThemeFilter"/>
    <images.themeFilter implementation="org.jetbrains.idea.devkit.images.DarculaHiDPIThemeFilter"/>
  </extensions>
</idea-plugin>
