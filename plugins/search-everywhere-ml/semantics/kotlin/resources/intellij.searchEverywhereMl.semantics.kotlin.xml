<idea-plugin package="com.intellij.searchEverywhereMl.semantics.kotlin">
  <dependencies>
    <plugin id="org.jetbrains.kotlin"/>
    <module name="intellij.searchEverywhereMl.semantics"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij.searcheverywhere.ml">
    <classesProvider filetype="Kotlin" implementationClass="com.intellij.searchEverywhereMl.semantics.kotlin.KotlinClassesProvider"/>
    <symbolsProvider filetype="Kotlin" implementationClass="com.intellij.searchEverywhereMl.semantics.kotlin.KotlinSymbolsProvider"/>
  </extensions>
</idea-plugin>