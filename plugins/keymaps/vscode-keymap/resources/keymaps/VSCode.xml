  </action>
  <action id="ForceRefresh" />
  <action id="Rerun">
    <keyboard-shortcut first-keystroke="ctrl SEMICOLON" second-keystroke="l" />
    <keyboard-shortcut first-keystroke="ctrl shift f5" />
  </action>
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="ctrl SEMICOLON" second-keystroke="a" />
  </action>
  <action id="RerunFailedTests">
    <keyboard-shortcut first-keystroke="ctrl SEMICOLON" second-keystroke="e" />
  </action>
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="ctrl i" />
    <keyboard-shortcut first-keystroke="ctrl space" />
  </action>
  <action id="CloseAllEditorsButActive">
    <!-- for "Close saved editors in group", no better alternative -->
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="u"  />
  </action>
  <action id="MoveEditorToOppositeTabGroup">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="PinActiveTabAction">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="shift enter" />
  </action>
  <action id="HideActiveWindow">
    <keyboard-shortcut first-keystroke="ctrl j"  />
  </action>
  <action id="ActivateProblemsViewToolWindow">
    <keyboard-shortcut first-keystroke="ctrl shift m"  />
  </action>
  <action id="PreviousProjectWindow" />
  <action id="CollapseBlock">
    <keyboard-shortcut first-keystroke="ctrl alt OPEN_BRACKET" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl l" />
  </action>
  <action id="CollapseDocComments">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl SLASH"/>
  </action>
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="control Y"/>
    <keyboard-shortcut first-keystroke="control shift Z"/>
    <keyboard-shortcut first-keystroke="alt shift BACK_SPACE"/>
  </action>
</keymap>