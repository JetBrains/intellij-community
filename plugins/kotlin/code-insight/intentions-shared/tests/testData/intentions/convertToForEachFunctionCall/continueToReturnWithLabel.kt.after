// WITH_STDLIB
// AFTER-WARNING: Label is redundant, because it can not be referenced in either 'break', 'continue', or 'return' expression
fun main() {
    outer@
    <caret>(1..100).forEach { i ->
        if (i % 2 == 0) return@forEach
        inner@
        for (j in 1..100) {
            continue@inner
        }
        for (j in 1..100) {
            for (k in 1..1) {
                return@forEach
            }
            continue
        }
    }
}