// WITH_STDLIB
class MyClass(val property: String)

fun test(obj: Any) {
    when (obj) {
        is MyClass if obj.property == "a" -> {
            println("a")
        }

        is MyClass if obj.property == "b" -> {
            println("b")
        }

        else -> {
            println("other")
        }
    }
}
