// COMPILER_ARGUMENTS: -Xcontext-parameters
// K2_AFTER_ERROR: Multiple potential context arguments for 'p: String' in scope.

context(p: String) fun foo() {
    p.substring(1)
    println(p == p)
}

context(c: String)
fun String.bar() {
    foo()
    with("baz") {
        foo()
    }
    with(this) {
        foo()
    }
    "boo".run {
        with(this) {
            foo()
        }
    }
}
