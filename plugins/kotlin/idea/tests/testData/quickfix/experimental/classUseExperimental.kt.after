// "Opt in for 'MyExperimentalAPI' on containing class 'Bar'" "true"
// PRIORITY: HIGH
// COMPILER_ARGUMENTS: -opt-in=kotlin.RequiresOptIn
// WITH_RUNTIME

package a.b

@RequiresOptIn
@Target(AnnotationTarget.FUNCTION)
annotation class MyExperimentalAPI

@MyExperimentalAPI
fun foo() {}

@OptIn(MyExperimentalAPI::class)
class Bar {
    fun bar() {
        foo()
    }
}
