// PARAM_TYPES: kotlin.Int
// PARAM_TYPES: kotlin.Int
// PARAM_DESCRIPTOR: value-parameter a: kotlin.Int defined in foo
// PARAM_DESCRIPTOR: val b: kotlin.Int defined in foo
// SIBLING:
fun foo(a: Int): Int {
    val b: Int = 1
    if (b(a, b)) return 0

    return 1
}

private fun b(a: Int, b: Int): Boolean {
    when (a + b) {
        0 -> return true
        1 -> println(1)
        else -> println(2)
    }
    return false
}