// WITH_STDLIB
// PARAM_DESCRIPTOR: val z: kotlin.Int defined in baz
// PARAM_TYPES: kotlin.Int
@RequiresOptIn
annotation class Marker

@RequiresOptIn
annotation class Another

@Marker
fun foo(x: Int): Int = x

@Another
fun bar(x: Int) {
    println(x)
}

@Marker
@Another
fun baz() {
    val z = foo(1)
    __dummyTestFun__(z)
}

@Another
private fun __dummyTestFun__(z: Int) {
    println(z)
    bar(z)
}
