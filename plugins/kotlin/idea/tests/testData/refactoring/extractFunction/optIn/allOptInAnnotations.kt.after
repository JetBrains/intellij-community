// WITH_STDLIB
@RequiresOptIn
annotation class Marker

@RequiresOptIn
annotation class Another

@Marker
fun foo(x: Int): Int = x

@Another
fun bar(x: Int) {
    println(x)
}

@OptIn(Marker::class, Another::class)
fun baz() {
    __dummyTestFun__()
}

@OptIn(Marker::class, Another::class)
private fun __dummyTestFun__() {
    bar(foo(1))
}
