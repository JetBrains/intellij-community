// SUGGESTED_NAMES: s, getX
// PARAM_DESCRIPTOR: value-parameter a: kotlin.Int defined in foo
// PARAM_TYPES: kotlin.Int
fun foo(a: Int): String {
    val x = """_${string(a)}_"""
    val y = "_$a:${a + 1}d_"
    val z = """_c$a:${a + 1}d_"""
    val u = "_c$a\n:${a + 1}d_"
    return """ab${string(a)}ef"""
}

private fun string(a: Int): String = """c$a
    :${a + 1}d"""
