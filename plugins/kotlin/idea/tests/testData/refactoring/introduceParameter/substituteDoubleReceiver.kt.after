// WITH_DEFAULT_VALUE: false
public inline fun <T, R> with(receiver: T, f: T.() -> R): R = receiver.f()

class A(val a: Int) {
    fun foo(i: Int): Int {
        return i / 2
    }

    fun test() {
        val a1 = A(1)
        foo(a + a1.a)
    }
}

fun test() {
    val t = with(A(1)) {
        val a1 = A(2)
        foo(a + a1.a)
    }
}