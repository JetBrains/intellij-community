                                               implementationClass="org.jetbrains.kotlin.idea.completion.impl.k2.contributors.commands.KotlinRenameActionCommandProvider"/>
    <codeInsight.completion.command.provider language="kotlin"
                                               implementationClass="com.intellij.codeInsight.completion.command.commands.OptimizeImportCompletionCommandProvider"/>
    <codeInsight.completion.command.provider language="kotlin"
                                               implementationClass="com.intellij.codeInsight.completion.command.commands.LineCommentCompletionCommandProvider"/>
    <codeInsight.completion.command.provider language="kotlin"
