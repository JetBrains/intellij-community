<idea-plugin package="org.jetbrains.kotlin.idea.debugger.sequence">
  <dependencies>
    <plugin id="org.jetbrains.debugger.streams"/>
  </dependencies>

  <extensions defaultExtensionNs="org.jetbrains.platform.debugger.streams">
    <librarySupport implementation="org.jetbrains.kotlin.idea.debugger.sequence.lib.java.JavaStandardLibrarySupportProvider"/>
