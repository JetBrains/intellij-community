<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.intellij.sh.shellcheck.ShellcheckOptionsPanel">
  <grid id="27dc6" binding="myPanel" layout-manager="GridLayoutManager" row-count="4" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="0" left="0" bottom="0" right="5"/>
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="52a7a" class="javax.swing.JLabel">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text resource-bundle="messages/ShBundle" key="sh.path.label"/>
          <verticalAlignment value="0"/>
          <verticalTextPosition value="0"/>
        </properties>
      </component>
      <component id="ca2ba" class="com.intellij.openapi.ui.TextFieldWithBrowseButton" binding="myShellcheckSelector">
        <constraints>
          <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties/>
      </component>
      <grid id="edbc2" binding="myWarningPanel" layout-manager="FlowLayout" hgap="0" vgap="0" flow-align="1">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="9" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <enabled value="true"/>
          <visible value="false"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="2514b" class="javax.swing.JLabel" binding="myWarningLabel">
            <constraints/>
            <properties>
              <text resource-bundle="messages/ShBundle" key="sh.invalid.path"/>
            </properties>
          </component>
          <component id="156ed" class="com.intellij.ui.components.ActionLink" binding="myShellcheckDownloadLink" custom-create="true">
            <constraints/>
            <properties>
              <text resource-bundle="messages/ShBundle" key="sh.download"/>
            </properties>
          </component>
          <component id="6ea2d" class="javax.swing.JLabel">
            <constraints/>
            <properties>
              <text resource-bundle="messages/ShBundle" key="sh.shellcheck.missing"/>
            </properties>
          </component>
        </children>
      </grid>
      <scrollpane id="1511d" class="com.intellij.ui.components.JBScrollPane">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="1" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties/>
        <border type="empty"/>
        <children>
          <grid id="e1434" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="0" left="0" bottom="0" right="0"/>
            <constraints/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="5a622" class="com.intellij.codeInspection.ui.MultipleCheckboxOptionsPanel" binding="myInspectionsCheckboxPanel" custom-create="true">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
              </component>
            </children>
          </grid>
        </children>
      </scrollpane>
      <component id="90c76" class="com.intellij.ui.components.JBLabel" binding="myErrorLabel">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="9" fill="0" indent="1" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text resource-bundle="messages/ShBundle" key="sh.shellcheck.cannot.download"/>
          <visible value="false"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
