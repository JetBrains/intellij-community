String quarter = switch (month) {
    case JANUARY:
    case FEBRUARY:
    case MARCH:
        yield "First Quarter"
    case APRIL:
    case MAY:
    case JUNE:
        yield "Second Quarter"
    case JULY:
    case AUGUST:
    case SEPTEMBER:
        yield "Third Quarter"
    case OCTOBER:
    case NOVEMBER:
    case DECEMBER:
        yield "Forth Quarter"
    default:
        yield "Unknown Quarter"
}
