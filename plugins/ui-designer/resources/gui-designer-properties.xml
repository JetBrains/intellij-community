<component>
  <class name="javax.swing.JComponent">
    <expert-properties>
      <property name="autoscrolls"/>
      <property name="name"/>
      <property name="opaque"/>
      <property name="focusable"/>
      <property name="focusCycleRoot"/>
      <property name="debugGraphicsOptions"/>
      <property name="doubleBuffered"/>
      <property name="verifyInputWhenFocusTarget"/>
      <property name="requestFocusEnabled"/>
      <property name="visible"/>
      <property name="inheritsPopupMenu"/>
      <property name="nextFocusableComponent"/>
      <property name="focusTraversalPolicyProvider"/>
      <property name="alignmentX"/>
      <property name="alignmentY"/>
      <property name="minimumSize"/>
      <property name="maximumSize"/>
      <property name="preferredSize"/>
    </expert-properties>
  </class>
  
  <class name="javax.swing.AbstractButton">  
    <expert-properties>
      <property name="actionCommand"/>
      <property name="label"/>
      <property name="margin"/>
      <property name="focusPainted"/>
      <property name="iconTextGap"/>
      <property name="borderPainted"/>
      <property name="contentAreaFilled"/>
      <property name="rolloverEnabled"/>
      <property name="disabledIcon"/>
      <property name="disabledSelectedIcon"/>
      <property name="pressedIcon"/>
      <property name="rolloverIcon"/>
      <property name="rolloverSelectedIcon"/>
      <property name="selectedIcon"/>
      <property name="multiClickThreshhold"/>
    </expert-properties>
    <inplace-property name="text"/>

    <enum-properties>
      <property name="horizontalAlignment">
        <constant value="2" message="property.left"/>
        <constant value="0" message="property.center"/>
        <constant value="4" message="property.right"/>
        <constant value="10" message="property.leading"/>
        <constant value="11" message="property.trailing"/>
      </property>
      <property name="horizontalTextPosition">
        <constant value="2" message="property.left"/>
        <constant value="0" message="property.center"/>
        <constant value="4" message="property.right"/>
        <constant value="10" message="property.leading"/>
        <constant value="11" message="property.trailing"/>
      </property>
      <property name="verticalAlignment">
        <constant value="1" message="property.top"/>
        <constant value="0" message="property.center"/>
        <constant value="3" message="property.bottom"/>
      </property>
      <property name="verticalTextPosition">
        <constant value="1" message="property.top"/>
        <constant value="0" message="property.center"/>
        <constant value="3" message="property.bottom"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JButton">
    <expert-properties>
      <property name="selected"/>
      <property name="defaultCapable"/>
    </expert-properties>
  </class>

  <class name="javax.swing.JCheckBox">
    <expert-properties>
      <property name="borderPaintedFlat"/>
    </expert-properties>
  </class>

  <class name="javax.swing.JLabel">
    <inplace-property name="text"/>

    <expert-properties>
      <property name="iconTextGap"/>
      <property name="disabledIcon"/>
    </expert-properties>

    <enum-properties>
      <property name="horizontalAlignment">
        <constant value="2" message="property.left"/>
        <constant value="0" message="property.center"/>
        <constant value="4" message="property.right"/>
        <constant value="10" message="property.leading"/>
        <constant value="11" message="property.trailing"/>
      </property>
      <property name="horizontalTextPosition">
        <constant value="2" message="property.left"/>
        <constant value="0" message="property.center"/>
        <constant value="4" message="property.right"/>
        <constant value="10" message="property.leading"/>
        <constant value="11" message="property.trailing"/>
      </property>
      <property name="verticalAlignment">
        <constant value="1" message="property.top"/>
        <constant value="0" message="property.center"/>
        <constant value="3" message="property.bottom"/>
      </property>
      <property name="verticalTextPosition">
        <constant value="1" message="property.top"/>
        <constant value="0" message="property.center"/>
        <constant value="3" message="property.bottom"/>
      </property>
    </enum-properties>
  </class>
  
  <class name="javax.swing.text.JTextComponent">
    <inplace-property name="text"/>
    <expert-properties>
      <property name="caretPosition"/>
      <property name="dragEnabled"/>
      <property name="selectionEnd"/>
      <property name="selectionStart"/>
      <property name="margin"/>
      <property name="caretColor"/>
      <property name="disabledTextColor"/>
      <property name="selectedTextColor"/>
      <property name="selectionColor"/>
    </expert-properties>
  </class>

  <class name="javax.swing.JTextField">
    <expert-properties>
      <property name="scrollOffset"/>
    </expert-properties>
    <enum-properties>
      <property name="horizontalAlignment">
        <constant value="2" message="property.left"/>
        <constant value="0" message="property.center"/>
        <constant value="4" message="property.right"/>
        <constant value="10" message="property.leading"/>
        <constant value="11" message="property.trailing"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JTextArea">
    <expert-properties>
      <property name="tabSize"/>
    </expert-properties>
  </class>

  <class name="javax.swing.JComboBox">
    <expert-properties>
      <property name="popupVisible"/>
      <property name="lightWeightPopupEnabled"/>
      <property name="selectedIndex"/>
      <property name="actionCommand"/>
      <property name="maximumRowCount"/>
    </expert-properties>
    <inplace-property name="model"/>
  </class>

  <class name="javax.swing.JSplitPane">
    <expert-properties>
      <property name="lastDividerLocation"/>
    </expert-properties>

    <enum-properties>
      <property name="orientation">
        <constant value="0" message="property.vertical"/>
        <constant value="1" message="property.horizontal"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JTable">
    <expert-properties>
      <property name="editingColumn"/>
      <property name="editingRow"/>
      <property name="surrendersFocusOnKeystroke"/>
      <property name="rowHeight"/>
      <property name="rowMargin"/>
      <property name="cellSelectionEnabled"/>
      <property name="columnSelectionAllowed"/>
      <property name="rowSelectionAllowed"/>
      <property name="autoCreateColumnsFromModel"/>
      <property name="dragEnabled"/>
    </expert-properties>

    <enum-properties>
      <property name="autoResizeMode">
        <constant value="0" message="property.resize.off"/>
        <constant value="1" message="property.resize.next.column"/>
        <constant value="2" message="property.resize.subsequent.columns"/>
        <constant value="3" message="property.resize.last.column"/>
        <constant value="4" message="property.resize.all.columns"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JTree">
    <expert-properties>
      <property name="invokesStopCellEditing"/>
      <property name="largeModel"/>
      <property name="rowHeight"/>
      <property name="toggleClickCount"/>
      <property name="visibleRowCount"/>
      <property name="dragEnabled"/>
      <property name="expandsSelectedPaths"/>
      <property name="scrollsOnExpand"/>
    </expert-properties>
  </class>

  <class name="javax.swing.JList">
    <expert-properties>
      <property name="dragEnabled"/>
      <property name="visibleRowCount"/>
      <property name="valueIsAdjusting"/>
      <property name="fixedCellHeight"/>
      <property name="fixedCellWidth"/>
      <property name="selectedIndex"/>
    </expert-properties>

    <enum-properties>
      <property name="layoutOrientation">
        <constant value="0" message="property.vertical"/>
        <constant value="1" message="property.vertical.wrap"/>
        <constant value="2" message="property.horizontal.wrap"/>
      </property>
      <property name="selectionMode">
        <constant value="0" message="property.selection.single"/>
        <constant value="1" message="property.selection.single.interval"/>
        <constant value="2" message="property.selection.multiple.interval"/>
      </property>
    </enum-properties>

    <inplace-property name="model"/>
  </class>

  <class name="javax.swing.JScrollPane">
    <expert-properties>
      <property name="wheelScrollingEnabled"/>
    </expert-properties>

    <enum-properties>
      <property name="horizontalScrollBarPolicy">
        <constant value="30" message="property.as.needed"/>
        <constant value="31" message="property.never"/>
        <constant value="32" message="property.always"/>
      </property>
      <property name="verticalScrollBarPolicy">
        <constant value="20" message="property.as.needed"/>
        <constant value="21" message="property.never"/>
        <constant value="22" message="property.always"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JTabbedPane">
    <enum-properties>
      <property name="tabLayoutPolicy">
        <constant value="0" message="property.wrap"/>
        <constant value="1" message="property.scroll"/>
      </property>
      <property name="tabPlacement">
        <constant value="1" message="property.top"/>
        <constant value="2" message="property.left"/>
        <constant value="3" message="property.bottom"/>
        <constant value="4" message="property.right"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JSlider">
    <enum-properties>
      <property name="orientation">
        <constant value="0" message="property.horizontal"/>
        <constant value="1" message="property.vertical"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JFormattedTextField">
    <enum-properties>
      <property name="focusLostBehavior">
        <constant value="0" message="property.focuslost.commit"/>
        <constant value="1" message="property.focuslost.commit.or.revert"/>
        <constant value="2" message="property.focuslost.revert"/>
        <constant value="3" message="property.focuslost.persist"/>
      </property>
      <property name="horizontalAlignment">
        <constant value="2" message="property.left"/>
        <constant value="0" message="property.center"/>
        <constant value="4" message="property.right"/>
        <constant value="10" message="property.leading"/>
        <constant value="11" message="property.trailing"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JSeparator">
    <enum-properties>
      <property name="orientation">
        <constant value="0" message="property.horizontal"/>
        <constant value="1" message="property.vertical"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JProgressBar">
    <enum-properties>
      <property name="orientation">
        <constant value="0" message="property.horizontal"/>
        <constant value="1" message="property.vertical"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JToolBar">
    <enum-properties>
      <property name="orientation">
        <constant value="0" message="property.horizontal"/>
        <constant value="1" message="property.vertical"/>
      </property>
    </enum-properties>
  </class>

  <class name="javax.swing.JScrollBar">
    <enum-properties>
      <property name="orientation">
        <constant value="0" message="property.horizontal"/>
        <constant value="1" message="property.vertical"/>
      </property>
    </enum-properties>
  </class>
</component>
