      <categoryKey>intention.category.declaration</categoryKey>
    </intentionAction>

    <intentionAction>
      <className>com.siyeh.ipp.varargs.WrapVarargArgumentsWithExplicitArrayIntention</className>
      <categoryKey>intention.category.declaration</categoryKey>
