<idea-plugin package="com.intellij.ide.startup.importSettings">
  <actions>
    <!--suppress PluginXmlI18n -->
    <action id="ProductChooserDialogAction" class="com.intellij.ide.startup.importSettings.chooser.productChooser.ProductChooserDialogAction" text="[INTERNAL] Transfer: Show Product Chooser Dialog" internal="true"/>
    <action id="ProductChooserSingleDialogAction" class="com.intellij.ide.startup.importSettings.chooser.productChooser.ProductChooserSingleDialogAction" text="[INTERNAL] Transfer: Show Product Chooser Dialog Single" internal="true"/>
    <!--suppress PluginXmlI18n -->
    <action id="SettingChooserDialogAction" class="com.intellij.ide.startup.importSettings.chooser.settingChooser.SettingChooserDialogAction" text="[INTERNAL] Transfer: Show Setting Chooser Dialog" internal="true"/>
    <action id="SettingChooserSimpleDialogAction" class="com.intellij.ide.startup.importSettings.chooser.settingChooser.SettingChooserSimpleDialogAction" text="[INTERNAL] Transfer: Show Setting Chooser Dialog Single" internal="true"/>

    <action id="ImportProgressDialogAction" class="com.intellij.ide.startup.importSettings.chooser.importProgress.ImportProgressDialogAction" text="[INTERNAL] Transfer: Import Progress Dialog" internal="true"/>
    <action id="ImportProgressSingleDialogAction" class="com.intellij.ide.startup.importSettings.chooser.importProgress.ImportProgressSingleDialogAction" text="[INTERNAL] Transfer: Import Progress Dialog Single" internal="true"/>


  </actions>

  <extensions defaultExtensionNs="com.intellij">
    <!--suppress PluginXmlDynamicPlugin -->
    <ideStartupWizard implementation="com.intellij.ide.startup.importSettings.IdeStartupWizardImpl"/>

    <applicationService serviceInterface="com.intellij.ide.startup.importSettings.data.SettingsService"
                        serviceImplementation="com.intellij.ide.startup.importSettings.data.SettingsServiceImpl"/>
  </extensions>
</idea-plugin>
