{
  "name": "razor",
  "version": "1.0.0",
  "description": "%description%",
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "razor",
        "extensions": [
          ".cshtml",
          ".razor"
        ],
        "aliases": [
          "Razor",
          "razor"
        ],
        "mimetypes": [
          "text/x-cshtml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "razor",
        "scopeName": "text.html.cshtml",
        "path": "./syntaxes/cshtml.tmLanguage.json",
        "embeddedLanguages": {
          "section.embedded.source.cshtml": "csharp",
          "source.css": "css",
          "source.js": "javascript"
        }
      }
    ]
  }
}
