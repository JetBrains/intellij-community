{
	"comments": {
		"lineComment": "REM"
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		{ "open": "\"", "close": "\"", "notIn": ["string"] }
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["%", "%"],
		["\"", "\""]
	],
	"folding": {
		"markers": {
			"start": "^\\s*(::\\s*|REM\\s+)#region",
			"end": "^\\s*(::\\s*|REM\\s+)#endregion"
		}
	}
}
