{
  "name": "language-viml",
  "version": "1.2.1",
  "description": "Vim Script language support for Atom",
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "VimL",
        "extensions": [
          ".vim",
          ".vimrc",
          ".gvimrc",
          ".nvimrc",
          "_vimrc",
          ".vmb",
          ".ideavimrc"
        ],
        "aliases": [
          "vim",
          "viml",
          "vim script"
        ]
      }
    ],
    "grammars": [
      {
        "language": "VimL",
        "scopeName": "source.viml",
        "path": "./grammars/viml.json"
      }
    ]
  }
}
