{
  "name": "restructuredtext",
  "version": "1.0.0",
  "description": "%description%",
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "restructuredtext",
        "aliases": [
          "reStructuredText"
        ],
        "configuration": "./language-configuration.json",
        "extensions": [
          ".rst"
        ]
      }
    ],
    "grammars": [
      {
        "language": "restructuredtext",
        "scopeName": "source.rst",
        "path": "./syntaxes/rst.tmLanguage.json"
      }
    ]
  }
}
