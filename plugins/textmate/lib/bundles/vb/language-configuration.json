{
	"comments": {
		"lineComment": "'"
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["<", ">"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		{ "open": "\"", "close": "\"", "notIn": ["string"] }
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["<", ">"]
	],
	"folding": {
		"markers": {
			"start": "^\\s*#Region\\b",
			"end": "^\\s*#End Region\\b"
		}
	}
}
