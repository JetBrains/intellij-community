
{
	"injectionSelector": "L:comment.block.documentation",
	"patterns": [
		{
			"include": "#jsdocbody"
		}
	],
	"repository": {
		"jsdocbody": {
			"begin": "(?<=/\\*\\*)([^*]|\\*(?!/))*$",
			"while": "(^|\\G)\\s*\\*(?!/)(?=([^*]|[*](?!/))*$)",
			"patterns": [
				{
					"include": "source.ts#docblock"
				}
			]
		}
	},
	"scopeName": "documentation.injection.js.jsx"
}
