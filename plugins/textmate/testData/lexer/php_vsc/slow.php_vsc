<?php
session_start();
require '../vendor/autoload.php';

use GuzzleHttp\Client;
use PHPMailer\PHPMailer\PHPMailer;

if (isset($_SESSION['username']) && !empty($_SESSION['username'])) {
    $role = $_SESSION['userrole'];
    $displayName = $_SESSION['userdisplay'];
    $username = $_SESSION['username'];
    $image = $_SESSION['userimage'];
    $personalEmail = $_SESSION['useremail'];
    $epmsLogIn = $_SESSION['empsusername'];

    $client = new Client();
    $estimateNumber = $_POST['estno'];
    $sellLevelCount = count($_POST['selllevel']);
    $sellPriceCount = count($_POST['sellprice']);

    for ($i = 0; $i < $sellLevelCount; $i++) {
        $sellLevel[] = $_POST['selllevel'][$i];
        $sellPrice[] = $_POST['sellprice'][$i];
    }


    try {
        $quoteLetterAPIURL = "http://192.168.102.84:7000/enterprise/v1.0/quoteletterapi/$estimateNumber";
        $quoteLetterAPIResp = $client->request('GET', $quoteLetterAPIURL);
        $quoteLetterAPIData = json_decode($quoteLetterAPIResp->getBody());

        $customerName = $quoteLetterAPIData->estimatedetails[0]->CustName;
        $customerContact = $quoteLetterAPIData->estimatedetails[0]->CustContact;
        $customerAddress = $quoteLetterAPIData->estimatedetails[0]->CustAddress1 . ' ' . $quoteLetterAPIData->estimatedetails[0]->CustAddress2;
        $customerCityStateZip = $quoteLetterAPIData->estimatedetails[0]->CustCity . ', ' . $quoteLetterAPIData->estimatedetails[0]->CustState . ' ' . $quoteLetterAPIData->estimatedetails[0]->CustZip;
        $customerTerms = $quoteLetterAPIData->estimatedetails[0]->Terms;
        $quoteSchedule = $quoteLetterAPIData->estimatedetails[0]->FormNumber;
        $projectName = $quoteLetterAPIData->estimatedetails[0]->JobDescription;
        $salesRepName = $quoteLetterAPIData->estimatedetails[0]->SalesName;
        $compCount = count($quoteLetterAPIData->estcomptext);

        if (count($quoteLetterAPIData->estcomptext) == 1 || isset($_POST['combine'])) {
            $pagesAndSize[] = $quoteLetterAPIData->estcomptext[0]->PagesInstruction;
            $proofInstruction[] = $quoteLetterAPIData->estcomptext[0]->ProofInstruction;
            $inkAndCoating[] = $quoteLetterAPIData->estcomptext[0]->InkCoating;
            $stockInstruction[] = $quoteLetterAPIData->estcomptext[0]->Stock;
            $finishingInstruction[] = $quoteLetterAPIData->estcomptext[0]->Finishing;
            $packingAndDelivery[] = $quoteLetterAPIData->estcomptext[0]->PackDelivery;
            $groupName[] = $quoteLetterAPIData->estcomptext[0]->GroupName;

        } else {

            for ($i = 0; $i < count($quoteLetterAPIData->estcomptext); $i++) {
//                $pagesAndSize[] = $quoteLetterAPIData->estcomptext[$i]->PagesInstruction;
                $pagesAndSize[] = str_replace("\r\n", "<br>", $quoteLetterAPIData->estcomptext[$i]->PagesInstruction);
                $proofInstruction[] = $quoteLetterAPIData->estcomptext[$i]->ProofInstruction;
                $inkAndCoating[] = $quoteLetterAPIData->estcomptext[$i]->InkCoating;
                $stockInstruction[] = $quoteLetterAPIData->estcomptext[$i]->Stock;
//                $finishingInstruction[] = str_replace("\r\n", "<br>", $quoteLetterAPIData->estcomptext[$i]->Finishing);
                $finishingInstruction[] = $quoteLetterAPIData->estcomptext[$i]->Finishing;
                $packingAndDelivery[] = $quoteLetterAPIData->estcomptext[$i]->PackDelivery;
                $groupName[] = $quoteLetterAPIData->estcomptext[$i]->GroupName;
            }

            //trim(preg_replace) removes white space from strings
            //htmlspecialchars ENTQUOTES removes escaping quotes from middle of string
            for ($i = 0; $i < count($pagesAndSize); $i++) {
                $pagesAndSize[$i] = trim(preg_replace('/\s+/', ' ', $pagesAndSize[$i]));
                $pagesAndSize[$i] = htmlspecialchars($pagesAndSize[$i], ENT_QUOTES);
            }

            for ($i = 0; $i < count($inkAndCoating); $i++) {
                $inkAndCoating[$i] = trim(preg_replace('/\s+/', ' ', $inkAndCoating[$i]));
            }
            for ($i = 0; $i < count($stockInstruction); $i++) {
                $stockInstruction[$i] = trim(preg_replace('/\s+/', ' ', $stockInstruction[$i]));
            }
            for ($i = 0; $i < count($finishingInstruction); $i++) {
                $finishingInstruction[$i] = trim(preg_replace('/\s+/', ' ', $finishingInstruction[$i]));
                $finishingInstruction[$i] = htmlspecialchars($finishingInstruction[$i], ENT_QUOTES);
            }
            for ($i = 0; $i < count($packingAndDelivery); $i++) {
                $packingAndDelivery[$i] = trim(preg_replace('/\s+/', ' ', $packingAndDelivery[$i]));
            }
        }
    } catch (\GuzzleHttp\Exception\GuzzleException $e) {
    }
}
