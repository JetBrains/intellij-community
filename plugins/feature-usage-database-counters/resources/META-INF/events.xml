<!-- Events should be registered in this file -->
<idea-plugin>
  <extensions defaultExtensionNs="com.intellij.ae.database">
    <fusEventCatcher implementation="com.intellij.ae.database.counters.community.events.AltEnterActionInvocation$Factory"/>

    <fusEventCatcher implementation="com.intellij.ae.database.counters.community.events.PluginInstalledFusListener"/>
    <fusEventCatcher implementation="com.intellij.ae.database.counters.community.events.PluginUninstalledFusListener"/>
  </extensions>

  <extensions defaultExtensionNs="com.intellij">
    <vfs.asyncListener implementation="com.intellij.ae.database.counters.community.events.TestFileCreationListener"/>
    <editorFactoryListener implementation="com.intellij.ae.database.counters.community.events.CodingTimeUserActivityEditorFactoryListener"/>
    <postStartupActivity implementation="com.intellij.ae.database.counters.community.events.BuildListenerProjectActivity"/>
    <checkinHandlerFactory implementation="com.intellij.ae.database.counters.community.events.VcsListenerFactory"/>
    <postStartupActivity implementation="com.intellij.ae.database.counters.community.events.ProjectOpenedTimeSpanUserActivityProjectOpenListener"/>
  </extensions>

  <applicationListeners>
    <listener class="com.intellij.ae.database.counters.community.events.IdeStartedUserActivityListener"
              topic="com.intellij.ide.AppLifecycleListener"/>
    <listener class="com.intellij.ae.database.counters.community.events.CompletionCharactersSparedListener"
              topic="com.intellij.featureStatistics.FeatureStatisticsUpdateListener"/>
    <listener class="com.intellij.ae.database.counters.community.events.RunConfigurationListener"
              topic="com.intellij.execution.ExecutionListener"/>
    <listener class="com.intellij.ae.database.counters.community.events.PluginInstalledUserActivityServiceInitializer"
              topic="com.intellij.ide.AppLifecycleListener"/>
    <listener class="com.intellij.ae.database.counters.community.events.ProjectOpenedTimeSpanUserActivityProjectCloseListener"
              topic="com.intellij.openapi.project.ProjectCloseListener"/>
    <listener class="com.intellij.ae.database.counters.community.events.CodingTimeUserActivityCommandListener"
              topic="com.intellij.openapi.command.CommandListener"/>
  </applicationListeners>
</idea-plugin>