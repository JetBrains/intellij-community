<idea-plugin xmlns:xi="http://www.w3.org/2001/XInclude" implementation-detail="true">
  <xi:include href="/META-INF/events.xml" xpointer="xpointer(/idea-plugin/*)" />

  <id>com.jetbrains.ae.database.counters.community</id>
  <name>Feature Usage Counters (Community)</name>
  <description>Register usage of various IDE features: VCS, code editing, run configuration, etc..</description>
  <vendor>JetBrains</vendor>

  <incompatible-with>com.intellij.cwm.guest</incompatible-with>
  <incompatible-with>com.intellij.jetbrains.client</incompatible-with>
  <incompatible-with>com.intellij.gateway</incompatible-with>

  <depends>com.intellij.modules.platform</depends>
  <depends>com.jetbrains.ae.database</depends>
</idea-plugin>