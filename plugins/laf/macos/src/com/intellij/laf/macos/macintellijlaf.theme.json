{
  "name": "macOS Light",
  "dark": false,
  "author": "JetBrains",

  "ui": {
    "*": {
      "_comment": "All *.selectionBackground colors are redefined to native ones in MacIntelliJLaf.kt",
      "selectionBackgroundInactive": "#D8D8D8",
      "selectionForeground": "#ffffff",
      "selectionForegroundInactive": "#000000",
      "selectionInactiveBackground": "#D8D8D8"
    },

    "ActionButton": {
      "hoverBackground": "#dadada",
      "hoverBorderColor": "#dadada",
      "pressedBackground": "#c9c9c9",
      "pressedBorderColor": "#c9c9c9"
    },

    "BookmarkMnemonicAssigned": {
      "background": "#F7C777",
      "borderColor": "#F7C777",
      "foreground": "#000000"
    },

    "BookmarkMnemonicAvailable": {
      "background": "#FFFFFF",
      "borderColor": "#C4C4C4",
      "foreground": "#000000"
    },

    "Borders.ContrastBorderColor": "#c9c9c9",

    "Button": {
      "background": "#ececec",
      "border": "com.intellij.laf.macos.MacIntelliJButtonBorder",
      "UI": "com.intellij.laf.macos.MacIntelliJButtonUI"
    },

    "CellEditor.border.width": 2,

    "CheckBox": {
      "background": "#ececec",
      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaCheckBoxBorder",
      "borderInsets": "4,4,4,0",
      "UI": "com.intellij.laf.macos.MacIntelliJCheckBoxUI"
    },

    "ComboBox": {
      "background": "#ffffff",
      "border": "com.intellij.laf.macos.MacIntelliJComboBoxBorder",
      "disabledBackground": "#ececec",
      "padding": "1,6,1,6",
      "UI": "com.intellij.laf.macos.MacIntelliJComboBoxUI"
    },

    "CompletionPopup.selectionBackground": "#c5dffc",

    "Component": {
      "errorFocusColor": "#f53b3b80",
      "focusColor": "#479cfc80",
      "focusWidth": 4,
      "inactiveErrorFocusColor": "#f7bcbc80",
      "inactiveWarningFocusColor": "#ffda9980",
      "warningFocusColor": "#e9ad4380"
    },

    "BookmarkMnemonicCurrent": {
      "background": "#389FD6",
      "borderColor": "#389FD6",
      "foreground": "#FFFFFF"
    },

    "EditorTabs.underlinedTabBackground": "#fafafa",
    "HelpTooltip": {
      "descriptionSizeDelta": -1,
      "mouseCursorOffset": 17,
      "xOffset": 1,
      "yOffset": 1
    },

    "Label.background": "#ececec",

    "Link": {
      "activeForeground": "#2470B3",
      "hoverForeground": "#2470B3",
      "pressedForeground": "#0058a6"
    },

    "Menu": {
      "background": "#f6f6f6",
      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaMenuItemBorder",
      "borderColor": "#f6f6f6",
      "separatorColor": "#dfdfdf"
    },

    "MenuItem": {
      "acceleratorForeground": "#808080",
      "acceleratorSelectionForeground": "#dcdcdc",
      "background": "#f6f6f6",
      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaMenuItemBorder"
    },

    "Bookmark": {
      "iconBackground": "#F7C777",
      "Mnemonic": {
        "iconForeground": "#000000",
        "iconBackground": "#FEF7EC",
        "iconBorderColor": "#F4AF3D"
      }
    },

    "NavBar.borderColor": "#cdcdcd",
    "OptionButtonUI": "com.intellij.laf.macos.MacIntelliJOptionButtonUI",
    "Panel.background": "#ececec",

    "PasswordField": {
      "border": "com.intellij.laf.macos.MacIntelliJTextBorder",
      "UI": "com.intellij.laf.macos.MacIntelliJPasswordFieldUI"
    },

    "Popup": {
      "Header.activeBackground": "#e0e0e0",
      "Header.inactiveBackground": "#ebebeb",
      "Toolbar.background": "#f5f5f5",
      "Toolbar.borderColor": "#f5f5f5",
      "borderColor": "#bebebe",
      "inactiveBorderColor": "#bebebe"
    },

    "PopupMenu": {
      "background": "#f6f6f6",
      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaPopupMenuBorder",
      "borderInsets": "6,1,6,1",
      "borderCornerRadius": 8,
      "borderWidth": 0,
      "translucentBackground": "#ececec",
      "UI": "com.intellij.ide.ui.laf.intellij.IdeaPopupMenuUI"
    },

    "PopupMenuSeparator": {
      "height": 9,
      "stripeWidth": 1,
      "stripeIndent": 4,
      "withToEdge": 10,
      "UI": "com.intellij.ide.ui.laf.darcula.ui.DarculaMenuSeparatorUI"
    },

    "ProgressBar": {
      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaProgressBarBorder",
      "failedColor": "#d64f4f",
      "failedEndColor": "#fb8f89",
      "indeterminateEndColor": "#0089fc",
      "indeterminateStartColor": "#86c4ff",
      "passedColor": "#34b171",
      "passedEndColor": "#7ee8a5",
      "progressColor": "#0089fc",
      "trackColor": "#d4d4d4",
      "UI": "com.intellij.laf.macos.MacIntelliJProgressBarUI"
    },

    "RadioButton": {
      "background": "#ececec",
      "border": "com.intellij.ide.ui.laf.darcula.ui.DarculaRadioButtonBorder",
      "borderInsets": "3,3,3,0",
      "UI": "com.intellij.laf.macos.MacIntelliJRadioButtonUI"
    },

    "ScrollBar.background": "#F5F5F5",

    "Spinner": {
      "background": "#ececec",
      "border": "com.intellij.laf.macos.MacIntelliJSpinnerBorder",
      "darcula.disabledButtonColor": "#ececec",
      "UI": "com.intellij.laf.macos.MacIntelliJSpinnerUI"
    },

    "SplitPane.highlight": "#ececec",

    "StatusBar": {
      "borderColor": "#c9c9c9",
      "hoverBackground": "#dadada"
    },

    "TabbedPane": {
      "background": "#ececec",
      "contentAreaColor": "#bfbfbf",
      "disabledUnderlineColor": "#ababab",
      "focusColor": "#dae4ed",
      "hoverColor": "#d9d9d9",
      "labelShift": 0,
      "selectedLabelShift": 0,
      "selectedTabPadInsets": "0,0,0,0",
      "tabAreaInsets": "0,0,0,0",
      "tabFillStyle": "underline",
      "tabHeight": 32,
      "tabInsets": "0,12,0,12",
      "tabSelectionHeight": 3,
      "tabsOverlapBorder": true,
      "underlineColor": "#4083C9",
      "UI": "com.intellij.ide.ui.laf.darcula.ui.DarculaTabbedPaneUI"
    },

    "TableHeader": {
      "background": "#F2F2F2",
      "bottomSeparatorColor": "#d6d6d6",
      "cellBorder": "2,2,2,2",
      "separatorColor": "#d6d6d6"
    },

    "TextField": {
      "border": "com.intellij.laf.macos.MacIntelliJTextBorder",
      "disabledBackground": "#ececec",
      "UI": "com.intellij.laf.macos.MacIntelliJTextFieldUI"
    },


    "SearchFieldWithExtension": {
      "border": "com.intellij.laf.macos.MacIntelliSearchBorder",
      "UI": "com.intellij.ui.components.DarculaSearchFieldWithExtensionUI"
    },

    "Tree": {
      "hash": "#E6E6E6",
      "paintLines": false
    },

    "MainToolbar": {
      "Icon.borderInsets": "10,10,10,10",
      "Dropdown.borderInsets": "12,6,12,6"
    },

    "window": "#ececec"
  }
}