<idea-plugin package="com.intellij.laf.win10">
  <id>com.intellij.laf.win10</id>
  <name>Windows 10 Light Theme</name>
  <vendor>JetBrains</vendor>
  <category>Themes</category>

  <description><![CDATA[Provides Windows 10 Look And Feel that mimics Windows 10 system UI]]></description>

  <dependencies>
    <plugin id="com.intellij.modules.lang"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="win10Light" path="/win10intellijlaf.theme.json"  targetUi="classic" parentTheme="IntelliJ"/>
  </extensions>
</idea-plugin>