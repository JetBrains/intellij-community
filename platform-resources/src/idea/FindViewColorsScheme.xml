<component>
  <scheme name="FindView">
    <colors>
    </colors>

    <attributes>
      <!-- "INVALID" prefix before invalidated usage -->
      <option name="$INVALID_PREFIX">
        <value>
          <option name="FOREGROUND" value="FF0000" />
          <option name="BACKGROUND" />
          <option name="EFFECT_TYPE" />
          <option name="EFFECT_COLOR" />
          <option name="FONT_TYPE" />
        </value>
      </option>

      <!-- "Read-only" prefix before read-only usage -->
      <option name="$READ_ONLY_PREFIX">
        <value>
          <option name="FOREGROUND" value="FF0000" />
          <option name="BACKGROUND" />
          <option name="EFFECT_TYPE" />
          <option name="EFFECT_COLOR" />
          <option name="FONT_TYPE" />
        </value>
      </option>

      <!-- Directory node which has read-only child -->
      <option name="$HAS_READ_ONLY_CHILD">
        <value>
          <option name="FOREGROUND" value="FF0000" />
          <option name="BACKGROUND" />
          <option name="EFFECT_TYPE" />
          <option name="EFFECT_COLOR" />
          <option name="FONT_TYPE" />
        </value>
      </option>

      <!-- Node with text only -->
      <option name="$TEXT_NODE">
        <value>
          <option name="FOREGROUND" />
          <option name="BACKGROUND" />
          <option name="EFFECT_TYPE" />
          <option name="EFFECT_COLOR" />
          <option name="FONT_TYPE" value="1" />
        </value>
      </option>

      <!-- Number of usages comment for classes, methods, files -->
      <option name="$NUMBER_OF_USAGES">
        <value>
          <option name="FOREGROUND" value="808080" />
          <option name="BACKGROUND" />
          <option name="EFFECT_TYPE" />
          <option name="EFFECT_COLOR" />
          <option name="FONT_TYPE" value="2" />
        </value>
      </option>

      <!-- Occurence -->
      <option name="$OCCURENCE">
        <value>
          <option name="FOREGROUND" />
          <option name="BACKGROUND" />
          <option name="EFFECT_TYPE" />
          <option name="EFFECT_COLOR" />
          <option name="FONT_TYPE" value="1" />
        </value>
      </option>

      <!-- Selected occurence -->
      <option name="$SELECTED_OCCURENCE">
        <value>
          <option name="FOREGROUND" />
          <option name="BACKGROUND" />
          <option name="EFFECT_TYPE" />
          <option name="EFFECT_COLOR" />
          <option name="FONT_TYPE" value="1" />
        </value>
      </option>

      <!-- Excluded node -->
      <option name="$EXCLUDED_NODE">
        <value>
          <option name="FOREGROUND" />
          <option name="BACKGROUND" />
          <option name="EFFECT_TYPE" value="3" />
          <option name="EFFECT_COLOR" value="808080" />
          <option name="FONT_TYPE" />
        </value>
      </option>
    </attributes>

  </scheme>
</component>