  <xs:element name="XmlDataSet">
    <xs:annotation>
      <xs:documentation>This class maps an xml file outputted from an ADO.NET DataSet to aBiDataSet.
      </xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice>
        <xs:group ref="BindowsComponentsGroup" minOccurs="0" maxOccurs="unbounded"/>
