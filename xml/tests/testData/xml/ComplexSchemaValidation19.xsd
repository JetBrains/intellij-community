      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="onshow" type="xs:string">
      <xs:annotation>
        <xs:documentation>This is fired when the calendar is shown.</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name="onhide" type="xs:string">
