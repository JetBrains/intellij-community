    </xs:attribute>
    <xs:attribute name="checked" type="xs:string">
      <xs:annotation>
        <xs:documentation>Type:Boolean - Whether the item is checked or not.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="onchange" type="xs:string">
      <xs:annotation>
