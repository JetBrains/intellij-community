      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="onframeprogression" type="xs:string">
      <xs:annotation>
        <xs:documentation>Fired upon every new frame</xs:documentation>
      </xs:annotation>
    </xs:attribute>


  </xs:attributeGroup>

