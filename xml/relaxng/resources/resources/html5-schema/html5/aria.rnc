## ARIA

## Global states and properties

aria.global =
	(	aria.prop.atomic?
	&	aria.state.busy?
	&	aria.prop.controls?
	&	aria.state.current?
	&	aria.prop.describedby?
	&	aria.prop.details?
	&	aria.state.disabled?
	&	aria.state.dropeffect?
	&	aria.prop.errormessage?
	&	aria.prop.flowto?
	&	aria.state.grabbed?
	&	aria.prop.haspopup?
	&	aria.state.hidden?
	&	aria.state.invalid?
	&	aria.prop.keyshortcuts?
	&	aria.prop.label?
	&	aria.prop.labelledby?
	&	aria.prop.live?
	&	aria.prop.owns?
	&	aria.prop.relevant?
	&	aria.prop.roledescription?
	&	aria.prop.description?
	)

common.attrs.other &= aria.global?

