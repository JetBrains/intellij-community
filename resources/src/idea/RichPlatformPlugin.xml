    <!-- Errors -->
    <!-- Show full error options configurable only in full IDEA - platform supports only default IDE profile for now -->
    <projectConfigurable provider="com.intellij.profile.codeInspection.ui.ProjectInspectionToolsConfigurableProvider"/>

    <!-- Project Structure -->
    <projectConfigurable instance="com.intellij.compiler.options.CompilerConfigurable" order="after project"/>
    <!-- Compiler -->

    <projectConfigurable instance="com.intellij.psi.templateLanguages.TemplateDataLanguageConfigurable"/>

    <!-- Scopes -->
    <!-- This is registered as a top-level configurable in IDEA, and as a service (accessible from certain dialogs) in platform. -->
    <projectConfigurable implementation="com.intellij.ide.util.scopeChooser.ScopeChooserConfigurable"/>

    <!-- Application Configurables -->

    <!-- Path Variables -->
