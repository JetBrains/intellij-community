<!-- Jupyter language -->
<idea-plugin>
  <extensions defaultExtensionNs="com.intellij">
    <fileType name="Jupyter"
              language="Jupyter"
              extensions="ipynb"
              implementationClass="com.intellij.jupyter.core.jupyter.JupyterFileType"
              fieldName="INSTANCE"/>
    <iconMapper mappingFile="JupyterCoreIconMappings.json"/>
  </extensions>
</idea-plugin>
